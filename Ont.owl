                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="&ontologiaRio;Process"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Siderurgy -->

    <Class rdf:about="&ontologiaRio;Siderurgy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Tank -->

    <Class rdf:about="&ontologiaRio;Tank">
        <equivalentClass>
            <Restriction>
                <onProperty rdf:resource="&ontologiaRio;hasCapacity"/>
                <allValuesFrom>
                    <rdfs:Datatype>
                        <onDatatype rdf:resource="&xsd;double"/>
                        <withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minExclusive rdf:datatype="&xsd;double">0.0</xsd:minExclusive>
                            </rdf:Description>
                        </withRestrictions>
                    </rdfs:Datatype>
                </allValuesFrom>
            </Restriction>
        </equivalentClass>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Textile -->

