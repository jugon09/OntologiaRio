                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasOutput"/>
                        <onClass rdf:resource="&ontologiaRio;WaterMass"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
