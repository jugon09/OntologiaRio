    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#BelongTo -->

    <ObjectProperty rdf:about="&ontologiaRio;BelongTo">
        <rdfs:domain rdf:resource="&ontologiaRio;Factory"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#applies -->

    <ObjectProperty rdf:about="&ontologiaRio;applies">
        <rdfs:range rdf:resource="&ontologiaRio;Treatment"/>
        <rdfs:domain rdf:resource="&ontologiaRio;TreatmentPlant"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#has -->

    <ObjectProperty rdf:about="&ontologiaRio;has">
        <rdfs:domain rdf:resource="&ontologiaRio;Factory"/>
        <rdfs:range rdf:resource="&ontologiaRio;Tank"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasInput -->

    <ObjectProperty rdf:about="&ontologiaRio;hasInput">
        <rdfs:domain rdf:resource="&ontologiaRio;Process"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasOutput -->

    <ObjectProperty rdf:about="&ontologiaRio;hasOutput">
        <rdfs:domain rdf:resource="&ontologiaRio;Process"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasStored -->

    <ObjectProperty rdf:about="&ontologiaRio;hasStored">
        <rdfs:domain rdf:resource="&ontologiaRio;Tank"/>
        <rdfs:range rdf:resource="&ontologiaRio;WaterMass"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#isMadeBy -->

    <ObjectProperty rdf:about="&ontologiaRio;isMadeBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&ontologiaRio;Factory"/>
        <rdfs:domain rdf:resource="&ontologiaRio;PouredWater"/>
        <inverseOf rdf:resource="&ontologiaRio;madeA"/>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#madeA -->

    <ObjectProperty rdf:about="&ontologiaRio;madeA">
        <rdfs:domain rdf:resource="&ontologiaRio;Factory"/>
        <rdfs:range rdf:resource="&ontologiaRio;PouredWater"/>
    </ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Volume -->

    <DatatypeProperty rdf:about="&ontologiaRio;Volume">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasCapacity -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasCapacity">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;Volume"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasCode -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasCode">
        <rdfs:domain rdf:resource="&ontologiaRio;Process"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasDBO -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasDBO">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;hasPollutant"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasDQO -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasDQO">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;hasPollutant"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasInternalPurifyingPlant -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasInternalPurifyingPlant">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&ontologiaRio;Factory"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasOutputDouble -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasOutputDouble">
        <rdfs:domain rdf:resource="&ontologiaRio;Process"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasPollutant -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasPollutant">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&ontologiaRio;WaterMass"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#hasVolume -->

    <DatatypeProperty rdf:about="&ontologiaRio;hasVolume">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;Volume"/>
        <rdfs:domain rdf:resource="&ontologiaRio;WaterMass"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#produceDBO -->

    <DatatypeProperty rdf:about="&ontologiaRio;produceDBO">
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;producePollutant"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#produceDQO -->

    <DatatypeProperty rdf:about="&ontologiaRio;produceDQO">
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;producePollutant"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#producePollutant -->

    <DatatypeProperty rdf:about="&ontologiaRio;producePollutant">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#reduceDBO -->

    <DatatypeProperty rdf:about="&ontologiaRio;reduceDBO">
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;reducePollutant"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#reduceDQO -->

    <DatatypeProperty rdf:about="&ontologiaRio;reduceDQO">
        <rdfs:subPropertyOf rdf:resource="&ontologiaRio;reducePollutant"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#reducePollutant -->

    <DatatypeProperty rdf:about="&ontologiaRio;reducePollutant">
        <rdfs:domain rdf:resource="&ontologiaRio;Treatment"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#takesHours -->

    <DatatypeProperty rdf:about="&ontologiaRio;takesHours">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&ontologiaRio;Treatment"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#week -->

    <DatatypeProperty rdf:about="http://www.w3.org/2006/time#week">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2006/time#weeks"/>
    </DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#weeks -->

    <DatatypeProperty rdf:about="http://www.w3.org/2006/time#weeks"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Chemical -->

    <Class rdf:about="&ontologiaRio;Chemical">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;produceDBO"/>
                        <allValuesFrom>
                            <rdfs:Datatype>
                                <onDatatype rdf:resource="&xsd;double"/>
                                <withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="&xsd;double">0.4</xsd:maxInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="&xsd;double">0.0</xsd:minInclusive>
                                    </rdf:Description>
                                </withRestrictions>
                            </rdfs:Datatype>
                        </allValuesFrom>
                    </Restriction>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;produceDQO"/>
                        <allValuesFrom>
                            <rdfs:Datatype>
                                <onDatatype rdf:resource="&xsd;double"/>
                                <withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="&xsd;double">0.4</xsd:maxInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="&xsd;double">0.0</xsd:minInclusive>
                                    </rdf:Description>
                                </withRestrictions>
                            </rdfs:Datatype>
                        </allValuesFrom>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#ClassifyWaterNormative -->

    <Class rdf:about="&ontologiaRio;ClassifyWaterNormative">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontologiaRio;Process"/>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasInput"/>
                        <onClass rdf:resource="&ontologiaRio;WaterMass"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                    </Restriction>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasOutput"/>
                        <onClass rdf:resource="&ontologiaRio;WaterMass"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="&ontologiaRio;Process"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#CleanWaterMass -->

    <Class rdf:about="&ontologiaRio;CleanWaterMass">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontologiaRio;WaterMass"/>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasDBO"/>
                        <allValuesFrom>
                            <rdfs:Datatype>
                                <onDatatype rdf:resource="&xsd;double"/>
                                <withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="&xsd;double">0.4</xsd:maxInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="&xsd;double">0.0</xsd:minInclusive>
                                    </rdf:Description>
                                </withRestrictions>
                            </rdfs:Datatype>
                        </allValuesFrom>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <disjointWith rdf:resource="&ontologiaRio;PollutedWaterMass"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Factory -->

    <Class rdf:about="&ontologiaRio;Factory">
        <equivalentClass>
            <Restriction>
                <onProperty rdf:resource="&ontologiaRio;has"/>
                <onClass rdf:resource="&ontologiaRio;Tank"/>
                <minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</minQualifiedCardinality>
            </Restriction>
        </equivalentClass>
        <equivalentClass>
            <Restriction>
                <onProperty rdf:resource="&ontologiaRio;BelongTo"/>
                <onClass rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
                <minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</minQualifiedCardinality>
            </Restriction>
        </equivalentClass>
        <rdfs:comment>La clase fábrica pertenece a mínimo una industria y contiene minímo un tanque. Así, cada fábrica le añade los contaminantes pertenecientes a su industria a la masa de agua que genera y la almacena en un tanque para que luego esta masa sea tratada por la planta de tratamiento.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Food -->

    <Class rdf:about="&ontologiaRio;Food">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#GenerateIndustryWaterMass -->

    <Class rdf:about="&ontologiaRio;GenerateIndustryWaterMass">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontologiaRio;Process"/>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasInput"/>
                        <onClass rdf:resource="&ontologiaRio;Factory"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                    </Restriction>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasOutput"/>
                        <onClass rdf:resource="&ontologiaRio;WaterMass"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="&ontologiaRio;Process"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#MergeWater -->

    <Class rdf:about="&ontologiaRio;MergeWater">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontologiaRio;Process"/>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasInput"/>
                        <onClass rdf:resource="&ontologiaRio;WaterMass"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</qualifiedCardinality>
                    </Restriction>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasOutput"/>
                        <onClass rdf:resource="&ontologiaRio;WaterMass"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="&ontologiaRio;Process"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Metallurgy -->

    <Class rdf:about="&ontologiaRio;Metallurgy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Paper -->

    <Class rdf:about="&ontologiaRio;Paper">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Plastic -->

    <Class rdf:about="&ontologiaRio;Plastic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/camilo/ontologies/2016/4/untitled-ontology-4#Industry"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#PollutedWaterMass -->

    <Class rdf:about="&ontologiaRio;PollutedWaterMass">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontologiaRio;WaterMass"/>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;hasDBO"/>
                        <allValuesFrom>
                            <rdfs:Datatype>
                                <onDatatype rdf:resource="&xsd;double"/>
                                <withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="&xsd;double">100.0</xsd:maxInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:minExclusive rdf:datatype="&xsd;double">0.4</xsd:minExclusive>
                                    </rdf:Description>
                                </withRestrictions>
                            </rdfs:Datatype>
                        </allValuesFrom>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="&ontologiaRio;WaterMass"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#PouredWater -->

    <Class rdf:about="&ontologiaRio;PouredWater">
        <equivalentClass>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ontologiaRio;WaterMass"/>
                    <Restriction>
                        <onProperty rdf:resource="&ontologiaRio;isMadeBy"/>
                        <onClass rdf:resource="&ontologiaRio;Factory"/>
                        <qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</qualifiedCardinality>
                    </Restriction>
                </intersectionOf>
            </Class>
        </equivalentClass>
        <rdfs:subClassOf rdf:resource="&ontologiaRio;WaterMass"/>
    </Class>
    


    <!-- http://www.semanticweb.org/camilo/ontologies/2016/4/ontologiaRio#Process -->

    <Class rdf:about="&ontologiaRio;Process">
        <equivalentClass>
            <Restriction>
                <onProperty rdf:resource="&ontologiaRio;hasInput"/>
